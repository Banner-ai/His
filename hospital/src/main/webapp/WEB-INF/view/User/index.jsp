<form class="form-inline definewidth m20" action="<c:url value='/User/index.do'/>" method="get">    
    用户名称：
    <input type="text" name="user_name" id="user_name"class="abc input-default" value="">&nbsp;&nbsp;  
    <button type="submit" class="btn btn-primary">查询</button>
</form>
<table class="table table-bordered table-hover definewidth m10">
   
